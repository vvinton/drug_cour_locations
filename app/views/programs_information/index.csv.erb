<%=
  CSV.generate do |csv|
    csv << [ 'State',
             'County',
             'Court Type',
             'Program Name',
             'Address',
             'City',
             'State',
             'Zip',
             'Website',
             'Latitude',
             'Longitude',
             'State Coordinator First Name',
             'State Coordinator Last Name',
             'State Coordinator Title',
             'State Coordinator Email',
             'State Coordinator Phone' ]
    @results.each do |result|
      csv << [ clean_csv_field(result.state),
               clean_csv_field(result.county),
               clean_csv_field(result.program_type),
               clean_csv_field(result.program_name),
               clean_csv_field(result.address),
               clean_csv_field(result.website),
               clean_csv_field(result.city),
               clean_csv_field(result.state),
               clean_csv_field(result.zip_code),
               clean_csv_field(result.lat),
               clean_csv_field(result.long),
               @all_state_coordinators.dig(result.state, 'first_name'),
               @all_state_coordinators.dig(result.state, 'last_name'),
               @all_state_coordinators.dig(result.state, 'title'),
               @all_state_coordinators.dig(result.state, 'email'),
               @all_state_coordinators.dig(result.state, 'phone') ]
    end
  end.html_safe
%>
